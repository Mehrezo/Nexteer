<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Nexteer\PoBundle\Entity\Delivery" table="delivery">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="prnumber" type="string" column="prnumber" length="100" nullable="false"/>
    <field name="datelivraison" type="date" column="datelivraison" nullable="false"/>
    <field name="delivery" type="string" column="delivery" length="100" nullable="false"/>
    <field name="fichierattachment" type="text" column="fichierattachment" nullable="false"/>
  </entity>
</doctrine-mapping>
