        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Id_delivery</th>
            <td>{{ entity.iddelivery }}</td>
        </tr>
        <tr>
            <th>Orderindex</th>
            <td>{{ entity.orderindex }}</td>
        </tr>
        <tr>
            <th>Quantite</th>
