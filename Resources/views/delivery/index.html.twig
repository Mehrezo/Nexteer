<h1>delivery list</h1>

<table class="records_list" class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Prnumber</th>
            <th>Datelivraison</th>
            <th>Delivery</th>
            <th>Fichierattachment</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('delivery_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.prnumber }}</td>
            <td>{% if entity.datelivraison %}{{ entity.datelivraison|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>{{ entity.delivery }}</td>
            <td>{{ entity.fichierattachment }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('delivery_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('delivery_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('delivery_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
